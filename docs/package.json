{
  "name": "esthetic-docs",
  "version": "1.0.0",
  "private": true,
  "description": "Ã†sthetic Documentation",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "rm -rf public && concurrently \"pnpm run tsup:watch\" \"pnpm run sass:watch\" \"pnpm run 11ty:watch\"",
    "build": "rm -rf public && pnpm run tsup:build && pnpm run sass:build && pnpm run 11ty:build && pnpm run postcss",
    "postcss": "postcss public/assets/style.css -r --no-map && rm -rf public/assets/style.css.map",
    "netlify:deploy": "netlify deploy",
    "netlify:serve": "netlify dev -c \"eleventy --watch --serve --watch\" --targetPort 8080",
    "11ty:watch": "eleventy --serve --watch",
    "11ty:build": "eleventy",
    "tsup:watch": "tsup --watch",
    "tsup:build": "tsup --minify",
    "sass:watch": "sass --load-path=node_modules/ src/assets/scss/stylesheet.scss public/assets/style.css --watch",
    "sass:build": "sass --load-path=node_modules/ src/assets/scss/stylesheet.scss public/assets/style.css --no-source-map",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": [
      "index.js",
      "docs/public"
    ],
    "extends": [
      "@liquify/eslint-config"
    ],
    "rules": {
      "no-prototype-builtins": "off",
      "multiline-ternary": "off"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@brixtol/bootstrap"
      ]
    }
  },
  "dependencies": {
    "@brixtol/bootstrap": "^1.4.3",
    "@hotwired/stimulus": "^3.2.1",
    "@panoply/viewports": "^0.0.3",
    "codejar": "^3.7.0",
    "esthetic": "link:..",
    "lazy-line-painter": "^1.9.6",
    "mergerino": "^0.4.0",
    "mithril": "^2.2.2",
    "moloko": "link:../../moloko",
    "prismjs": "^1.29.0",
    "relapse": "^0.5.0",
    "spx": "link:../../../../brixtol/packages/spx",
    "stickybits": "^3.7.11"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@brixtol/browserslist-config": "^1.1.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@liquify/eslint-config": "^1.2.0",
    "@liquify/prettier-config": "^1.2.1",
    "@liquify/stylelint-config": "^1.0.0",
    "@panoply/11ty": "^0.2.0",
    "@sardine/eleventy-plugin-tinyhtml": "^0.2.0",
    "@types/browser-sync": "^2.27.0",
    "@types/markdown-it": "^12.2.3",
    "@types/prismjs": "^1.26.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "cssnano": "^6.0.0",
    "eleventy-plugin-svg-sprite": "^2.3.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "language-literals": "^0.0.1",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^3.0.0",
    "netlify-cli": "^13.2.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.3"
  }
}
